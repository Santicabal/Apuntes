        <mxCell id="6dNgAjrjlPRhg_Ximcxs-22" value="Leve: tópicos. Retinoides/peróxido de benzoilo ± antibióticos &lt;br style=&quot;font-size: 13px;&quot;&gt;Moderado: tetraciclinas orales o isotretinoína oral &lt;br style=&quot;font-size: 13px;&quot;&gt;Acné fu/minans: asociar corticoides orales" style="connectable=0;recursiveResize=0;strokeColor=inherit;fillColor=inherit;whiteSpace=wrap;html=1;verticalAlign=top;align=left;fontSize=13;" vertex="1" parent="6dNgAjrjlPRhg_Ximcxs-20">
